local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Feyu's Driving Empire Trainer!", HidePremium = false, SaveConfig = true, ConfigFolder = "OHelp"})

local Tab = Window:MakeTab({
    Name = "Driving Empire",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

OrionLib:MakeNotification({
    Name = "Accordingly Executed, Enjoy!",
    Content = "DM FEYU.LVP WITH ISSUES",
    Image = "rbxassetid://4483345998",
    Time = 7
})

Tab:AddButton({
    Name = "AutoFarm",
    Callback = function()
              print("button pressed")
              loadstring(game:HttpGet("https://raw.githubusercontent.com/Marco8642/science/main/drivingempire", true))()    
      end    
})
